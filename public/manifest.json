{
    "version": "0.1",
    "manifest_version": 3,
    "name": "GithubMetrics",
    "description": "Fetches metrics on PR's",
    "icons": {
      "128": "./assets/logo.png"
    },
    "permissions": [
      "storage"
    ],
    "content_scripts": [
      {
        "js": ["content_script.js"],
        "matches": [
          "https://github.com/Addepar/*"
        ]
      }
    ]
  }